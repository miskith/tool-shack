{
  "name": "tool-shack",
  "version": "0.0.29",
  "description": "Collection of usefull functions to ease work",
  "author": {
    "name": "David My≈°ka",
    "url": "https://www.davidmyska.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miskith/tool-shack/issues"
  },
  "homepage": "https://www.davidmyska.com/tool-shack/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miskith/tool-shack.git"
  },
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "src/**/*.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && tsc -b .",
    "format": "pretty-quick",
    "generate-docs": "rm -rf docs && typedoc --name \"Tool-Shack\" --readme README.md src/*/index.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "typedoc": "^0.27.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
