{
  "name": "tool-shack",
  "version": "0.0.24",
  "description": "Collection of usefull functions to ease work",
  "author": {
    "name": "David My≈°ka",
    "url": "https://www.davidmyska.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miskith/tool-shack/issues"
  },
  "homepage": "https://www.davidmyska.com/tool-shack/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miskith/tool-shack.git"
  },
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "src/**/*.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && tsc -b .",
    "format": "pretty-quick",
    "generate-docs": "rm -rf docs && typedoc --name \"Tool-Shack\" --readme README.md src/*/index.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  }
}
